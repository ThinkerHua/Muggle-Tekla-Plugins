					<Style BasedOn="{StaticResource element}" TargetType="UIControls:WpfProfileCatalog">
						<Setter Property="HorizontalAlignment" Value="Center" />
						<Setter Property="VerticalAlignment" Value="Center" />
					</Style>
				</ResourceDictionary>
